{
  "name": "react-experiments",
  "version": "0.1.0",
  "repository": "iiegor/react-experiments",
  "license": "MIT",
  "scripts": {
    "pretest": "node node_modules/fbjs-scripts/node/check-dev-engines.js package.json",
    "start": "cross-env NODE_ENV=development gulp serve",
    "build": "cross-env NODE_ENV=production gulp",
    "lint": "eslint .",
    "test": "cross-env NODE_ENV=test jest"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0",
    "fbjs": "^0.8.0-alpha.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.35",
    "babel-eslint": "^4.1.3",
    "css-loader": "^0.23.1",
    "del": "^2.2.0",
    "envify": "^3.4.0",
    "eslint": "^1.5.1",
    "eslint-plugin-react": "^3.2.2",
    "express": "^4.13.4",
    "fbjs-scripts": "^0.6.0-alpha.1",
    "fs.extra": "*",
    "glob": "*",
    "gulp": "^3.9.0",
    "gulp-babel": "^5.1.0",
    "gulp-browserify-thin": "^0.1.5",
    "gulp-concat-css": "^2.2.0",
    "gulp-derequire": "^2.1.0",
    "gulp-flatten": "^0.2.0",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "jest-cli": "^0.9.0-fb1",
    "object-assign": "^2.0.0",
    "optimist": "0.6.0",
    "run-sequence": "^1.1.2",
    "style-loader": "^0.13.0",
    "through2": "^2.0.1",
    "vinyl-buffer": "^1.0.0",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1",
    "webpack-stream": "^3.0.0"
  },
  "devEngines": {
    "node": "4.x",
    "npm": "2.x"
  },
  "jest": {
    "rootDir": "",
    "scriptPreprocessor": "scripts/jest/preprocessor.js",
    "setupEnvScriptFile": "node_modules/fbjs-scripts/jest/environment.js",
    "persistModuleRegistryBetweenSpecs": true,
    "modulePathIgnorePatterns": [
      "<rootDir>/lib/",
      "<rootDir>/node_modules/"
    ],
    "preprocessorIgnorePatterns": [
      "<rootDir>/node_modules/"
    ],
    "testPathDirs": [
      "<rootDir>/src/"
    ],
    "testRunner": "node_modules/jest-cli/src/testRunners/jasmine/jasmine2.js",
    "unmockedModulePathPatterns": [
      "<rootDir>/node_modules/fbjs/node_modules/",
      "<rootDir>/node_modules/fbjs/lib/(?!(UserAgent.js$|UserAgentData.js$))",
      "<rootDir>/node_modules/fbjs-scripts/",
      "<rootDir>/node_modules/react/",
      "<rootDir>/node_modules/react-dom/"
    ]
  }
}
